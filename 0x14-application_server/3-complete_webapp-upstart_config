# copy of /etc/init/airbnb.conf
description "Gunicorn application server running hbnb4dynamic"

start on runlevel [2345]
stop on runlevel [!2345]

respawn
setuid julija
setgid julija

env HBNB_MYSQL_USER=hbnb_dev
env HBNB_MYSQL_PWD=hbnb_dev_pwd
env HBNB_MYSQL_HOST=localhost
env HBNB_MYSQL_DB=hbnb_dev_db
env HBNB_TYPE_STORAGE=db

script
	cd /home/julija/AirBnB_clone_v4
	exec gunicorn --bind 127.0.0.1:8004 web_dynamic.4-hbnb:app
end script